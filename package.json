{
  "name": "linter-jing",
  "main": "./lib/main.js",
  "version": "0.4.3",
  "description": "On-the-fly validation and well-formedness check of XML documents against RELAX NG (compact, XML), Schematron (1.5, ISO), W3C Schema (XSD 1.0) and DTD",
  "keywords": [
    "xml",
    "relaxng",
    "schematron",
    "xsd",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aerhard/linter-jing.git"
  },
  "bugs": "https://github.com/aerhard/linter-jing/issues",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Alexander Erhard (https://github.com/aerhard/)",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^5.0.1",
    "atom-package-deps": "^4.0.1",
    "cross-spawn": "^4.0.0",
    "lodash": "^4.13.1",
    "sax": "^1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-jasmine": "^1.8.1",
    "eslint-plugin-react": "^4.2.0"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "eslint": "eslint .",
    "test": "apm test",
    "build": "babel src --out-dir lib --presets es2015-loose",
    "watch": "babel src --watch --out-dir lib --presets es2015-loose"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": [
          "provideLinter"
        ]
      }
    }
  }
}
